{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "./build/server",
  "scripts": {
    "start": "db-migrate up && node .",
    "build": "rm -rf ./build && npx tsc",
    "dev": "db-migrate up && ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set ENV=test&& db-migrate --env test reset && db-migrate --env test up && npm run build && jasmine && db-migrate --env test reset",
    "jasmine": "jasmine",
    "lint": "eslint . --ext ts",
    "prettier": "prettier --config ./.prettierrc ./src/**/*.ts --write"
  },
  "author": "AbdallaM-Ibrahim",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "sequelize": "^6.31.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.16.0",
    "@types/pg": "^7.14.7",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9",
    "typescript": "~4.2.0"
  }
}
